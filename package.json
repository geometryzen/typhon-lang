{
    "name": "typhon-lang",
    "version": "1.0.6",
    "description": "Typhon = Python + Types + ECMAScript modules",
    "author": {
        "name": "David Geo Holmes",
        "email": "david.geo.holmes@gmail.com"
    },
    "browser": "./build/browser/index.js",
    "main": "./build/main/index.js",
    "module": "./build/module/index.js",
    "types": "./build/index.d.ts",
    "license": "MIT",
    "licenses": [
        {
            "type": "MIT",
            "url": "http://github.com/geometryzen/typhon-lang/blob/master/LICENSE"
        }
    ],
    "keywords": [],
    "repository": {
        "type": "git",
        "url": "git://github.com/geometryzen/typhon-lang.git"
    },
    "scripts": {
        "build": "trash build && python3 ./build.py gen && rollup -c",
        "lint": "eslint . --ext .ts",
        "test": "nyc ./node_modules/.bin/_mocha 'test/**/*.ts'",
        "docs": "npm run docs:typedoc && open-cli docs/index.html",
        "docs:typedoc": "trash docs && typedoc --options typedoc.json",
        "prepublishOnly": "node check_publish_env.js && npm run build && npm run lint && npm test && npm run docs"
    },
    "devDependencies": {
        "@istanbuljs/nyc-config-typescript": "^1.0.2",
        "@rollup/plugin-commonjs": "^21.0.1",
        "@rollup/plugin-node-resolve": "^13.1.3",
        "@rollup/plugin-typescript": "^8.3.0",
        "@types/chai": "^4.3.0",
        "@types/mocha": "^9.1.0",
        "@typescript-eslint/eslint-plugin": "^5.11.0",
        "@typescript-eslint/parser": "^5.11.0",
        "chai": "^4.3.6",
        "eslint": "^8.8.0",
        "mocha": "^9.2.0",
        "nyc": "^15.1.0",
        "open-cli": "^7.0.1",
        "rollup": "^2.67.1",
        "rollup-plugin-dts": "^4.1.0",
        "rollup-plugin-terser": "^7.0.2",
        "trash-cli": "^5.0.0",
        "ts-node": "^10.5.0",
        "tslib": "^2.3.1",
        "typedoc": "^0.22.11",
        "typescript": "^4.5.5"
    }
}